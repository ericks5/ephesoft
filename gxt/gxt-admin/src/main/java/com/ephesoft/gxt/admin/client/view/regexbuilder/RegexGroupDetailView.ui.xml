<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:gxt="urn:import:com.sencha.gxt.widget.core.client" xmlns:g='urn:import:com.google.gwt.user.client.ui'>

	<g:VerticalPanel width="100%" spacing="2">
		<g:HorizontalPanel width="100%">
			<g:cell>
				<gxt:ContentPanel headerVisible="true" bodyBorder="false"
					borders="false" resize="false" width="100%" ui:field="regexGroupContainer">
				</gxt:ContentPanel>
			</g:cell>
		</g:HorizontalPanel>
		<g:HorizontalPanel>
			<g:cell>
				<g:CheckBox ui:field="caseInsensetiveCheckBox"></g:CheckBox>
			</g:cell>
			<g:cell>
				<g:Label ui:field="caseInsensetiveLabel" />
			</g:cell>
		</g:HorizontalPanel>
		<g:HorizontalPanel>
			<g:cell>
				<g:CheckBox ui:field="startGroupCheckBox"></g:CheckBox>
			</g:cell>
			<g:cell>
				<g:Label ui:field="startGroupLabel" />
			</g:cell>
		</g:HorizontalPanel>
		<g:HorizontalPanel>
			<g:cell>
				<g:CheckBox ui:field="endGroupCheckBox"></g:CheckBox>
			</g:cell>
			<g:cell>
				<g:Label ui:field="endGroupLabel" />
			</g:cell>
		</g:HorizontalPanel>
		<g:HorizontalPanel>
			<g:cell>
				<g:CheckBox ui:field="captureGroupCheckBox"></g:CheckBox>
			</g:cell>
			<g:cell>
				<g:Label ui:field="captureGroupLabel" />
			</g:cell>
		</g:HorizontalPanel>
		<g:HorizontalPanel>
			<g:cell>
				<g:CheckBox ui:field="nonCaptureGroupCheckBox"></g:CheckBox>
			</g:cell>
			<g:cell>
				<g:Label ui:field="nonCaptureGroupLabel" />
			</g:cell>
		</g:HorizontalPanel>
		<g:HorizontalPanel>
			<g:cell>
				<g:CheckBox ui:field="applyQuantifierToGroupCheckBox"></g:CheckBox>
			</g:cell>
			<g:cell>
				<g:Label ui:field="applyQuantifierToGroupLabel" />
			</g:cell>
		</g:HorizontalPanel>

		<g:HorizontalPanel ui:field="horizontalLinePanel"
			height="0px" width="100%">
		</g:HorizontalPanel>
	</g:VerticalPanel>
</ui:UiBinder>
