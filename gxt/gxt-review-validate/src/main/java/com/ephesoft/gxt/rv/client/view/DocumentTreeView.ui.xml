<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:container="urn:import:com.sencha.gxt.widget.core.client.container"
	xmlns:gxt="urn:import:com.sencha.gxt.widget.core.client"
	xmlns:coreView="urn:import:com.ephesoft.gxt.core.client.view" xmlns:rv="urn:import:com.ephesoft.gxt.rv.client.widget">
	<gxt:ContentPanel headerVisible="false" ui:field= "documentTreeHeader">
		<g:VerticalPanel width="100%" height="100%"
			styleName="documentViewPanel" ui:field="documentViewPanel">
			<g:HorizontalPanel height="5%" styleName="buttonPanel"
				width="100%">
				<g:Button ui:field="draggableImageSwitchButton" />
			</g:HorizontalPanel>
			<g:ScrollPanel width="100%" height="95%" ui:field="treeScroll">
				<rv:DocumentTree ui:field="documentTree" />
			</g:ScrollPanel>
		</g:VerticalPanel>
	</gxt:ContentPanel>
</ui:UiBinder>