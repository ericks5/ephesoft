<?xml version='1.0' encoding='UTF-8'?>
<definitions xmlns="http://www.omg.org/spec/BPMN/20100524/MODEL"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema"
	xmlns:activiti="http://activiti.org/bpmn" xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI"
	xmlns:omgdc="http://www.omg.org/spec/DD/20100524/DC" xmlns:omgdi="http://www.omg.org/spec/DD/20100524/DI"
	typeLanguage="http://www.w3.org/2001/XMLSchema" expressionLanguage="http://www.w3.org/1999/XPath"
	targetNamespace="http://www.activiti.org/test">
	<process id="Document_Assembler_Module" isExecutable="true">
		<extensionElements>
			<activiti:executionListener event="start"
				delegateExpression="${moduleExecutionStartListener}"></activiti:executionListener>
			<activiti:executionListener event="end"
				delegateExpression="${moduleExecutionEndListener}"></activiti:executionListener>
		</extensionElements>
		<startEvent id="start" />
		<sequenceFlow sourceRef="start"
			targetRef="Document_Assembler_Plugin-Plugin" />
		<callActivity id="Document_Assembler_Plugin-Plugin"
			activiti:async="true" calledElement="Document_Assembler_Plugin">
			<extensionElements>
				<activiti:in source="batchInstanceID" target="batchInstanceID" />
				<activiti:in source="key" target="key" />
			</extensionElements>
		</callActivity>
		<sequenceFlow sourceRef="Document_Assembler_Plugin-Plugin"
			targetRef="Document_Assembler_Scripting_Plugin-Plugin" />
		<callActivity id="Document_Assembler_Scripting_Plugin-Plugin"
			activiti:async="true" calledElement="Document_Assembler_Scripting_Plugin">
			<extensionElements>
				<activiti:in source="batchInstanceID" target="batchInstanceID" />
				<activiti:in source="key" target="key" />
				<activiti:in target="scriptName" sourceExpression="ScriptDocumentAssembler" />
				<activiti:in target="backupFileName"
					sourceExpression="Document_Assembler_Scripting_Plugin" />
			</extensionElements>
		</callActivity>
		<sequenceFlow sourceRef="Document_Assembler_Scripting_Plugin-Plugin"
			targetRef="end" />
		<endEvent id="end" />
	</process>
</definitions>
